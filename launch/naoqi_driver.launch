<launch>

  <arg name="nao_ip"            default="$(optenv NAO_IP)" />
  <arg name="roscore_ip"        default="127.0.0.1" />
  <arg name="network_interface" default="eth0" />

  <node pkg="naoqi_rosbridge" type="alrosbridge_bin" name="naoqi_rosbridge" required="true" args="--qi-url=tcp://$(arg nao_ip):9559 $(arg roscore_ip) $(arg network_interface)" output="screen" />

</launch>
